{
  "name": "@figma/my-make-file",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "check": "bun run lint && bun run fmt:check && bun run knip",
    "fix:esbuild": "bun scripts/fix-vite-esbuild.mjs",
    "postinstall": "bun run fix:esbuild",
    "build": "bun run fix:esbuild && vite build",
    "dev": "bun run fix:esbuild && vite",
    "lint": "oxlint --format stylish .",
    "lint:fix": "oxlint --fix .",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "knip": "knip"
  },
  "dependencies": {
    "lucide-react": "0.487.0",
    "motion": "12.23.24",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-markdown": "^10.1.0",
    "react-router": "^7.13.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "4.1.12",
    "@vitejs/plugin-react": "4.7.0",
    "esbuild": "0.25.11",
    "knip": "^5.76.3",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.3",
    "typescript": "^5.9.3",
    "vite": "6.3.5"
  },
  "engines": {
    "bun": ">=1.1.0"
  },
  "packageManager": "bun@1.2.16",
  "pnpm": {
    "overrides": {
      "vite": "6.3.5"
    }
  }
}
